{
    "projects": {
      "frontend": {
        "src": "./frontend",
        "buildCommand": "npm run build",
        "outputDirectory": "./frontend/build"
      },
      "backend": {
        "src": "./backend",
        "buildCommand": "python app/main.py",
        "outputDirectory": "./backend"
      }
    },
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/backend/$1"
      }
    ]
  }
  